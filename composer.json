  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "monolog/monolog": "^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "twig/twig": "^3.0",
    "doctrine/orm": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
